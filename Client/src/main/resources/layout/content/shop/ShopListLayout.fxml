<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml" fx:id="contentView"
            fx:controller="hihi.content.shop.ShopListLayoutController">
    <center>
        <TableView fx:id="table">
            <columns>
                <TableColumn fx:id="selectedColumn" text="Selected"/>
                <TableColumn fx:id="uidColumn" text="Uid"/>
                <TableColumn fx:id="nameColumn" text="Name"/>
                <TableColumn fx:id="websiteColumn" text="Website"/>
                <TableColumn fx:id="addressColumn" text="Address"/>
                <TableColumn fx:id="emailColumn" text="E-mail"/>
                <TableColumn fx:id="phoneColumn" text="Phone"/>
                <TableColumn fx:id="memoColumn" text="Memo"/>
            </columns>
        </TableView>
    </center>
    <bottom>
        <HBox fx:id="bottomPanel" />
    </bottom>
</BorderPane>