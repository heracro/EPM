<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml" fx:id="contentView"
            fx:controller="hihi.content.delivery.DeliveryListLayoutController">
    <center>
        <TableView fx:id="table">
            <columns>
                <TableColumn fx:id="selectedColumn" text="Selected"/>
                <TableColumn fx:id="uidColumn" text="Uid"/>
                <TableColumn fx:id="materialNameColumn" text="Material"/>
                <TableColumn fx:id="qtyColumn" text="Qty"/>
                <TableColumn fx:id="unitColumn" text="Unit"/>
                <TableColumn fx:id="unitPriceColumn" text="Unit price"/>
                <TableColumn fx:id="totalPriceColumn" text="Total price"/>
                <TableColumn fx:id="plannedDateColumn" text="Planned Arrival"/>
            </columns>
        </TableView>
    </center>
    <bottom>
        <HBox fx:id="bottomPanel" />
    </bottom>
</BorderPane>