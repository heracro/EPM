<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml" fx:id="contentView"
            fx:controller="hihi.content.project.ProjectListLayoutController">
    <center>
        <TableView fx:id="table">
            <columns>
                <TableColumn fx:id="selectedColumn" text="Selected"/>
                <TableColumn fx:id="uidColumn" text="Uid"/>
                <TableColumn fx:id="nameColumn" text="Name"/>
                <TableColumn fx:id="plannedStartDateColumn" text="Planned Start"/>
                <TableColumn fx:id="plannedEndDateColumn" text="Planned End"/>
                <TableColumn fx:id="realStartDateColumn" text="Real Start"/>
                <TableColumn fx:id="realEndDateColumn" text="Real End"/>
                <TableColumn fx:id="statusColumn" text="Status"/>
                <TableColumn fx:id="causeColumn" text="Cause"/>
                <TableColumn fx:id="projectLocationColumn" text="Location"/>
            </columns>
        </TableView>
    </center>
    <bottom>
        <HBox fx:id="bottomPanel" />
    </bottom>
</BorderPane>