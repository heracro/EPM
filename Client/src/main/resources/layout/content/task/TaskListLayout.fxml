<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<BorderPane xmlns:fx="http://javafx.com/fxml" fx:id="contentView"
            fx:controller="hihi.content.task.TaskListLayoutController">
    <center>
        <TableView fx:id="table">
            <columns>
                <TableColumn fx:id="selectedColumn" text="Selected"/>
                <TableColumn fx:id="uidColumn" text="Uid"/>
                <TableColumn fx:id="nameColumn" text="Name"/>
                <TableColumn fx:id="dueDateColumn" text="Due date"/>
                <TableColumn fx:id="statusColumn" text="Status"/>
            </columns>
        </TableView>
    </center>
</BorderPane>
